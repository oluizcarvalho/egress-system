{
	"name": "egress-system",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"prepare": "husky || true",
		"p:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
		"p:fix": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}\""
	},
	"engines": {
		"node": ">=18.20.0"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^18.1.0",
		"@angular/cdk": "^18.1.3",
		"@angular/common": "^18.1.0",
		"@angular/compiler": "^18.1.0",
		"@angular/core": "^18.1.0",
		"@angular/forms": "^18.1.0",
		"@angular/platform-browser": "^18.1.0",
		"@angular/platform-browser-dynamic": "^18.1.0",
		"@angular/router": "^18.1.0",
		"@angular/service-worker": "^18.1.0",
		"@fortawesome/fontawesome-free": "^5.15.4",
		"@govbr-ds/core": "^3.5.1",
		"chart.js": "^4.3.0",
		"dayjs": "^1.11.12",
		"express": "^4.18.2",
		"ng2-charts": "^6.0.1",
		"ngx-editor": "^18.0.0",
		"ngx-mask": "^18.0.0",
		"ngx-skeleton-loader": "^9.0.0",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.14.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^18.1.1",
		"@angular/cli": "^18.1.1",
		"@angular/compiler-cli": "^18.1.0",
		"@types/express": "^4.17.17",
		"@types/jasmine": "~5.1.0",
		"@types/node": "^18.18.0",
		"husky": "^9.1.1",
		"jasmine-core": "~5.1.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"lint-staged": "^15.2.7",
		"prettier": "^3.3.3",
		"typescript": "~5.5.2"
	},
	"lint-staged": {
		"*.{js,css,md,ts,html,json,scss}": "prettier --write"
	}
}
