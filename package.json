{
	"name": "egress-system",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"prepare": "husky || true",
		"p:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
		"p:fix": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
		"compodoc:build": "compodoc -p tsconfig.doc.json",
		"compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
		"compodoc:serve": "compodoc -s",
		"doc": "compodoc -p tsconfig.doc.json -n \"Sistema de Egressos UFU\" --language pt-BR --disableCoverage --disableInternal --disableLifeCycleHooks --customLogo ./public/favicon/apple-touch-icon.png --customFavicon ./public/favicon/favicon.ico --output ./documentation",
		"test": "ng test",
		"test:coverage-local": "ng test --coverage --coverage-directory coverage",
		"test:coverage": "ng test --ci --json --coverage --test-location-in-results --output-file report.json",
		"test:watch": "ng test --watch"
	},
	"engines": {
		"node": ">=18.20.0"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^18.2.12",
		"@angular/cdk": "^18.2.13",
		"@angular/common": "^18.2.12",
		"@angular/compiler": "^18.2.12",
		"@angular/core": "^18.2.12",
		"@angular/forms": "^18.2.12",
		"@angular/platform-browser": "^18.2.12",
		"@angular/platform-browser-dynamic": "^18.2.12",
		"@angular/router": "^18.2.12",
		"@angular/service-worker": "^18.2.12",
		"@fortawesome/fontawesome-free": "^5.15.4",
		"@govbr-ds/core": "^3.6.1",
		"chart.js": "^4.4.7",
		"dayjs": "^1.11.12",
		"express": "^4.21.2",
		"ng2-charts": "^6.0.1",
		"ngx-editor": "^18.0.0",
		"ngx-mask": "^18.0.0",
		"ngx-skeleton-loader": "^9.0.0",
		"rxjs": "~7.8.0",
		"tslib": "^2.8.1",
		"zone.js": "~0.14.3"
	},
	"devDependencies": {
		"@angular-builders/jest": "^18.0.0",
		"@angular-devkit/build-angular": "^18.2.12",
		"@angular/cli": "^18.2.12",
		"@angular/compiler-cli": "^18.2.12",
		"@compodoc/compodoc": "^1.1.26",
		"@ngneat/spectator": "^19.0.0",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.5.13",
		"@types/node": "^18.19.101",
		"husky": "^9.1.1",
		"jest": "^29.7.0",
		"jest-preset-angular": "^14.2.4",
		"lint-staged": "^15.2.7",
		"prettier": "^3.3.3",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"typescript": "~5.5.2"
	},
	"lint-staged": {
		"*.{js,css,md,ts,html,json,scss}": "prettier --write"
	}
}
